        <ui:Button text="My Modes" parse-escape-sequences="true" display-tooltip-when-elided="true" name="myModes" enable-rich-text="true" class="button" style="font-size: 50%;" />
        <ui:Button text="quit" parse-escape-sequences="true" display-tooltip-when-elided="true" name="quit" enable-rich-text="true" class="button" style="font-size: 50%;" />
        <ui:VisualElement name="savedModesContainer" style="flex-grow: 1; position: absolute; left: 96px; top: 53px; right: 96px; bottom: 56px; background-color: rgb(29, 29, 29); border-top-left-radius: 20px; border-top-right-radius: 20px; border-bottom-right-radius: 20px; border-bottom-left-radius: 20px; border-left-color: rgb(125, 74, 24); border-right-color: rgb(125, 74, 24); border-top-color: rgb(125, 74, 24); border-bottom-color: rgb(125, 74, 24); border-top-width: 7px; border-right-width: 7px; border-bottom-width: 7px; border-left-width: 7px; overflow: hidden; display: none;">
