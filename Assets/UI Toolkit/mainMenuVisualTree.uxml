            <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" name="modeDetailsContainer" style="align-self: flex-end; position: relative; right: 0; width: 70%; height: 100%; justify-content: center; align-items: center;">
                <ui:Label tabindex="-1" text="Label" parse-escape-sequences="true" display-tooltip-when-elided="true" name="modeName" style="font-size: 60px; color: rgb(255, 255, 255); -unity-text-align: middle-center; -unity-text-outline-color: rgb(87, 63, 26); -unity-text-outline-width: 1px; align-self: center; -unity-font-style: bold;" />
                <ui:Label tabindex="-1" text="Label" parse-escape-sequences="true" display-tooltip-when-elided="true" name="modeDesc" class="ModeDetailsText" style="font-size: 50px; color: rgb(255, 255, 255); -unity-text-align: middle-center; -unity-text-outline-color: rgb(87, 63, 26); -unity-text-outline-width: 1px; align-self: center;" />
                <ui:Label tabindex="-1" text="Shapes:" parse-escape-sequences="true" display-tooltip-when-elided="true" enable-rich-text="true" name="modeShapesList" class="ModeDetailsText" />
                <ui:Button text="Play" parse-escape-sequences="true" display-tooltip-when-elided="true" name="playButton" focusable="true" style="width: 40%; height: 150px; font-size: 60px; -unity-font-style: bold; border-left-color: rgb(137, 84, 39); border-right-color: rgb(137, 84, 39); border-top-color: rgb(137, 84, 39); border-bottom-color: rgb(137, 84, 39); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px; border-top-left-radius: 20px; border-top-right-radius: 20px; border-bottom-right-radius: 20px; border-bottom-left-radius: 20px; background-color: rgb(67, 67, 67); color: rgb(255, 255, 255); align-self: center;" />
